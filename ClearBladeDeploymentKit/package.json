{
  "name": "testsys",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:service": "webpack -display-error-details --config webpack.config.js --config-name service",
    "build:widget": "webpack --config webpack.config.js --config-name widget",
    "build:library": "webpack --config webpack.config.js --config-name library",
    "start:dev": "npm-run-all --parallel start:watch start:server",
    "start:watch": "webpack --config webpack.config.js --config-name clearblade-hot-reload",
    "start:server": "clearblade-hot-reload start",
    "test": "jest",
    "test:watch": "jest --watchAll"
  },
  "author": "",
  "license": "ISC",
  "babel": {
    "presets": [
      "@babel/typescript",
      [
        "@babel/preset-env"
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      [
        "module-resolver",
        {
          "root": [
            "./src"
          ]
        }
      ]
    ]
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "setupFiles": [
      "./src/setupTests.ts"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@clearblade/clearblade-hot-reload": "^1.1.2",
    "@types/clearbladejs-server": "^1.1.1",
    "@types/jest": "^24.0.6",
    "@types/node": "^11.9.4",
    "@types/q": "^1.5.2",
    "@types/react": "^16.8.10",
    "axios": "^0.18.0",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-module-resolver": "^3.2.0",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.2",
    "chokidar": "^2.1.5",
    "express": "^4.16.4",
    "jest": "^24.1.0",
    "jest-cli": "^24.1.0",
    "npm-run-all": "^4.1.5",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "socket.io": "^2.2.0",
    "source-map-loader": "^0.2.4",
    "ts-jest": "^24.0.0",
    "typescript": "^3.3.3",
    "webpack": "^4.29.4",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "webpack-hot-middleware": "^2.24.3"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.3",
    "@material-ui/core": "^3.9.3"
  }
}
